{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
	"shell:allow-open",
    "core:default",
    "core:window:allow-set-size",
    "dialog:default",
    "opener:default",
    "process:default"
  ],
  "plugins": {
    "fs": {
      "scope": [
        "$RESOURCE/resources/**"
      ]
    },
    "process": {
      "exit": true
    }
  }
}
